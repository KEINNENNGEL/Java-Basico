package OrdenaStringPipe;

public class Main {
	
	public static void main(String[]args){
		String []valores={"8,7,5,6,7,5,3,2,7,6,8,2,3,7,6,4|9,3,8,5,7,4,6,5,8,7,4,6,5,9,8,3|9,8,4,6,5,8,4,7,5,9,8,4,7,3,5,|2,6,4,8,2,7,3,8,24,9,2,8,4,2,1"};
		
		OrdenaStringPipe cadena=new OrdenaStringPipe();
		String dato[]=cadena.OrdenaSPipe(valores);
		
		for(int i=0;i<dato.length;i++){
			System.out.println(dato[i]);
		}
	}

}
